{
  "name": "fontlay",
  "version": "1.1.0",
  "description": "A simple Google Fonts proxy and privacy-friendly. Prevent your users from being tracked and push your privacy.",
  "author": "Farid Nizam",
  "license": "Apache-2.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "prettier": "npx prettier --config .prettierrc --write \"src/**/*.js\"",
    "prettier:check": "npx prettier --check \"src/**/*.js\"",
    "deploy:prod": "pnpm build && pm2 start ecosystem.config.js",
    "deploy:dev": "pnpm build && pm2 start ecosystem.config.js --env development",
    "deploy:delete": "pm2 delete ecosystem.config.js"
  },
  "dependencies": {
    "@next/font": "13.1.6",
    "daisyui": "^2.51.0",
    "eslint": "8.34.0",
    "eslint-config-next": "13.1.6",
    "next": "13.1.6",
    "next-seo": "^6.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.7.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "next-sitemap": "^4.0.2",
    "next-themes": "^0.2.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "tailwindcss": "^3.2.7"
  }
}
